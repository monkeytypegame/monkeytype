{
  "name": "@monkeytype/release",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec 'npm run release-dry'",
    "dev-changelog": "nodemon ./src/buildChangelog.js",
    "release": "node ./src/index.js",
    "release-fe": "node ./src/index.js --fe",
    "release-be": "node ./src/index.js --be",
    "release-no-deploy": "node ./src/index.js --no-deploy",
    "release-dry": "node ./src/index.js --dry",
    "lint": "eslint \"./**/*.js\""
  },
  "devDependencies": {
    "@monkeytype/eslint-config": "workspace:*",
    "eslint": "8.57.0"
  },
  "bin": {
    "monkeytype-release": "./src/index.js"
  },
  "dependencies": {
    "@octokit/rest": "21.0.1",
    "dotenv": "16.4.5",
    "nodemon": "3.1.4",
    "readline-sync": "1.4.10"
  }
}
