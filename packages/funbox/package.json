    "chokidar": "3.6.0",
    "eslint": "8.57.1",
    "madge": "8.0.0",
    "tsup": "8.4.0",
    "typescript": "5.5.4",
    "vitest": "2.1.9"
  },
  "dependencies": {
    "@monkeytype/util": "workspace:*"
  },
  "exports": {
    ".": {
