    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-rate-limit": "6.2.1",
    "firebase-admin": "10.0.2",
    "helmet": "4.6.0",
    "joi": "17.6.0",
    "lodash": "4.17.21",
    "mongodb": "4.4.0",
    "node-fetch": "2.6.7",
    "node-object-hash": "2.3.10",
    "nodemon": "2.0.7",
    "path": "0.12.7",
    "prom-client": "14.0.1",
    "simple-git": "2.45.1",
    "string-similarity": "4.0.4",
    "swagger-stats": "0.99.2",
