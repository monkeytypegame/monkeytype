      "properties": {
        "acc": {
          "type": "number",
          "format": "double",
          "example": 94.44
        },
        "consistency": {
          "type": "number",
          "format": "double",
          "example": 75.98
        },
        "difficulty": {
          "type": "string",
          "example": "normal"
        },
        "lazyMode": {
          "type": "boolean",
          "example": false
        },
        "language": {
          "type": "string",
          "example": "english"
        },
        "punctuation": {
          "type": "boolean",
          "example": false
        },
        "raw": {
          "type": "number",
          "format": "double",
          "example": 116.6
        },
        "wpm": {
          "type": "number",
          "format": "double",
          "example": 107.6
        },
        "timestamp": {
